module smallgc #(//counter module
    SIZE = 25 : SIZE > 0 // Width of the output
  )(
    input reset,
    input clk,  // clock
    input rst,  // reset
    output out[5]
  ) {
dff counter[SIZE](.clk(clk), .rst(rst));
dff slow[5](.clk(clk), .rst(rst));
 
  always {
    counter.d = counter.q + 1;
    out = slow.q;
    
    if (counter.q == SIZEx{b1}){
      slow.d = slow.q + 1;
    }
    
    if (reset){
      counter.d = 0;
      slow.d = 0;
    }
    
}}
